<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jobapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="Whistler"
        android:roundIcon="@mipmap/logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.JobApplication">
        <activity android:name=".SearchUser"></activity>
        <activity android:name=".UpdateProfile" />
        <activity android:name=".Admin_Listing" />
        <activity android:name=".Admin_Page" />
        <activity android:name=".ListingActivity" />
        <activity android:name=".JobPage" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>